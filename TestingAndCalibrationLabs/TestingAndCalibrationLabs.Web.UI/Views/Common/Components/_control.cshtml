@model TestingAndCalibrationLabs.Web.UI.Models.Node<TestingAndCalibrationLabs.Web.UI.Models.LayoutDTO>

@if (Model.Value.UiPageMetadata.ControlCategoryName == "DataControl")
{
    @await Html.PartialAsync("~/Views/Common/Components/_dataControls.cshtml", Model)
}
else if (Model.Value.UiPageMetadata.ControlCategoryName == "NonDataControl")
{
    @await Html.PartialAsync("~/Views/Common/Components/_nondataControl.cshtml", Model)
}