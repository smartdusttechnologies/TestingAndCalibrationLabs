@model RecordDTO
@foreach (var item in Model.Layout)
{
    
    @if (item.Value.ControlCategoryName == "DataControl")
    {
        @Html.Partial("~/Views/Common/Components/_dataControls.cshtml",item )
    }
    else if(item.Value.ControlCategoryName == "NonDataControl"){
        @Html.Partial("~/Views/Common/Components/_nondataControl.cshtml",item)
    }
    
    
}