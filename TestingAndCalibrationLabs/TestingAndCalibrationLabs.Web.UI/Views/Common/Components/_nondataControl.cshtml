@model TestingAndCalibrationLabs.Web.UI.Models.Node<TestingAndCalibrationLabs.Web.UI.Models.LayoutDTO>
@if (@Model.Value.UiPageMetadata.UiControlTypeId == (int)UiControlType.grid && @Model.Value.UiPageMetadata.UiControlDisplayName != "MultiControlGrid")
{
    @await Html.PartialAsync("~/Views/Common/Components/Grid/_grid.cshtml", Model)

}
else if(Model.Value.UiPageMetadata.UiControlTypeName == "ReportTemplate")
{
    @await Html.PartialAsync("~/Views/Common/Components/ReportTemplate/_test.cshtml", Model)
}
else 
{
    @await Html.PartialAsync(Model.Value.UiPageMetadata.UiControlCategoryTypeTemplate, Model)
}