@model TestingAndCalibrationLabs.Web.UI.Models.UiPageMetadataDTO
<div class="row ms-1 me-1">
    <div class="col-lg-12 col-sm-12 col-xs-12 mt-3 form-element left-pd-input">
        <input class="form-input width-full" type="tel" id="@Model.UiControlTypeName@Model.Id" placeholder=" " name="@Model.UiControlTypeName@Model.Id" onchange="this.setAttribute('value',this.value)" maxlength="10"/>
        <label class="form-label" for="@Model.UiControlTypeName">@Model.UiControlDisplayName</label>
        <span asp-validation-for="@Model.UiControlDisplayName" class="text-danger"></span>
    </div>
</div>