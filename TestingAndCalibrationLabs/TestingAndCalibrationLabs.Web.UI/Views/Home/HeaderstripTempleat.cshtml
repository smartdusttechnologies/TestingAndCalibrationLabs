@model TestingAndCalibrationLabs.Web.UI.Models.HeaderstripModelDTO

<head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
</head>
<body>
    <nav class=" main-header navbar navbar-expand navbar-white navbar-light">
        <ul class="navbar-nav">
            <li class="nav-item">
                <a class="navbar-brand">
                    <img src="@Model.CompanyImageURl" height="45" alt="Smatdust Technologies Logo" loading="lazy" />
                </a>
            </li>
        </ul>
        <ul class="navbar-nav ml-auto">
            <li class="nav-item">
                <div class="icon12">
                    <div>
                        <i class="fas fa-headset"></i>
                        @Model.Contact
                    </div>
                    <div>
                        <i class="fa fa-envelope"></i>
                        @Model.Email
                    </div>
                </div>
            </li>
            <li class="nav-item">
                <span>
                    <img src="@Model.AwardImageURL1" height="40" />
                    <img src="@Model.AwardImageURL2" height="40" />
                    <img src="@Model.AwardImageURL3" height="40" />
                </span>
            </li>
            <li class="nav-item">
                <div class="icon123">
                    <div>
                        <a class="fa fa-linkedin fa-sm" role="button" href="@Model.linkedlnURL">
                        </a>
                    </div>
                    <div>
                        <a class="fa fa-facebook fa-sm" role="button" href="@Model.FacebookURL">
                        </a>
                    </div>
                </div>
            </li>
        </ul>
    </nav>
</body>






