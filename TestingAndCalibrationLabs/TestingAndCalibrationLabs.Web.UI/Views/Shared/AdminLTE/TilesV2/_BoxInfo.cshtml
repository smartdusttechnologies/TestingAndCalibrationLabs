@{
    var value = ViewData["Control"] as DashboardV2;

}
<div class="info-box mb-3 bg-warning">
    <span class="info-box-icon"><i class="fas fa-tag"></i></span>

    <div class="info-box-content">
        <span class="info-box-text">Inventory</span>
        <span class="info-box-number" id="inventory">5,200</span>
    </div>
   
</div>

<div class="info-box mb-3 bg-success">
    <span class="info-box-icon"><i class="far fa-heart"></i></span>

    <div class="info-box-content">
        <span class="info-box-text">Mentions</span>
        <span class="info-box-number" id="mention">92,050</span>
    </div>
   
</div>

<div class="info-box mb-3 bg-danger">
    <span class="info-box-icon"><i class="fas fa-cloud-download-alt"></i></span>

    <div class="info-box-content">
        <span class="info-box-text">Downloads</span>
        <span class="info-box-number" id="downloads">114,381</span>
    </div>
   
</div>

<div class="info-box mb-3 bg-info">
    <span class="info-box-icon"><i class="far fa-comment"></i></span>

    <div class="info-box-content">
        <span class="info-box-text">Direct Messages</span>
        <span class="info-box-number" id="messages">163,921</span>
    </div>
   
</div>

<script>

    var Data = JSON.parse('@Html.Raw(Json.Serialize(value.map))');

    document.getElementById("inventory").innerHTML = Data.inventory;
    document.getElementById("mention").innerHTML = Data.mention;
    document.getElementById("downloads").innerHTML = Data.downloads;
    document.getElementById("messages").innerHTML = Data.messages;

</script>